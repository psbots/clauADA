{
    "name": "claude-interpreter",
    "version": "1.0.0",
    "description": "Use claude with interpreter",
    "private": true,
    "scripts": {
        "build": "parcel build src/manifest.json src/sidepanel.html src/settings.html --config @parcel/config-webextension",
        "build2": "parcel build src/manifest.json --config @parcel/config-webextension && parcel build src/sidepanel.html --dist-dir dist/sidepanel --config @parcel/config-webextension && parcel build src/settings.html --dist-dir dist/settings --config @parcel/config-webextension"
    },
    "author": "",
