    "devDependencies": {
        "@parcel/config-webextension": "^2.9.3",
        "@types/chrome": "^0.0.242",
        "buffer": "^6.0.3",
        "parcel": "^2.9.3",
        "postcss": "^8.4.31",
        "preline": "^1.9.0",
        "process": "^0.11.10",
        "pyodide": "^0.26.1",
        "@pyscript/core": "^0.4.55",
        "tailwindcss": "^3.3.3",
        "url": "^0.11.1",
        "util": "^0.12.5",
        "vm-browserify": "^1.1.2"
    },
    "dependencies": {
        "@aws-sdk/client-bedrock-runtime": "^3.609.0"
    }
